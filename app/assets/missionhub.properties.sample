# the production mode. values development|test|production
ENVIRONMENT=development

# access token of the user to login as, requires development environment
LOGIN_AS=

# the market the application is deployed to. values: play|amazon|nook
MARKET=play

# missionhub api
API_URL=https://www.missionhub.com/apis
API_VERSION=v3

# missionhub surveys
SURVEY_URL=http://mhub.cc/surveys

# missionhub oauth
OAUTH_URL=https://www.missionhub.com/oauth
OAUTH_CLIENT_ID=
OAUTH_CLIENT_SECRET=
OAUTH_SCOPE=userinfo,contacts,contact_assignment,followup_comments,roles,organization_info,groups,communication

# caching
CACHE_FILE_ENABLED=true
CACHE_FILE_AGE=3d
CACHE_HTTP_ENABLED=true
CACHE_HTTP_SIZE=10m

# google analytics
ANALYTICS_ENABLED=false
ANALYTICS_KEY=UA-
ANALYTICS_DEBUG=true
ANALYTICS_DRY_RUN=true
ANAYLTICS_ANONYMIZE_IP=false

# android crash reporter
ACRA_ENABLED=false
ACRA_FORM_KEY=