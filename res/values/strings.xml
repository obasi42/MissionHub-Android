	<string name="guide_contacts_unassigned_msg">This view shows all contacts that have been collected but not assigned. All designated leaders can access this page and can self-assign contacts. If you tap ‘Assign to me’, this contact will move to ‘My Contacts’.</string>
	<string name="guide_contact">Contact Information</string>
