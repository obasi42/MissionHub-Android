<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="Widget.MissionHub">
    </style>

    <style name="Widget.MissionHub.ActionBar" parent="Widget.Sherlock.Light.ActionBar">
    </style>
    
    <style name="Widget.MissionHub.ListView">
    </style>

    <style name="Widget.MissionHub.ListView.Contact">
        <item name="android:background">@color/transparent</item>
        <item name="android:cacheColorHint">@color/transparent</item>
    </style>

    <style name="Widget.MissionHub.ListView.Item">
        <item name="android:background">@drawable/list_item_background</item>
        <item name="android:padding">@dimen/listview_item_padding</item>
        <item name="android:minHeight">@dimen/listview_item_min_height</item>
    </style>

    <style name="Widget.MissionHub.ListView.Item.Contact"></style>

    <style name="Widget.MissionHub.ListView.Item.Category">
        <item name="android:padding">@null</item>
        <item name="android:paddingLeft">@dimen/category_listview_item_padding_sides</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    
    <style name="Widget.MissionHub.ListView.Item.Category.TextAppearance">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
         <item name="android:textColor">@color/white</item>
    </style>
    
    
    <style name="Widget.MissionHub.ListView.Item.Navigation">
        <item name="android:padding">0dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    
    <style name="Widget.MissionHub.ListView.Item.Navigation.Logo" parent="Widget.MissionHub">
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
    </style>
    
    <style name="Widget.MissionHub.ListView.Item.Navigation.Title" parent="Widget.MissionHub">
        
    </style>
    
     <style name="Widget.MissionHub.ListView.Item.Navigation.Subtitle" parent="Widget.MissionHub">
        
    </style>
    
    <style name="TextAppearance.MissionHub" parent="@android:style/TextAppearance">
        
        
    </style>
    
    <style name="Widget.MissionHub.VPI"></style>

    <style name="Widget.MissionHub.VPI.TitlePageIndicator" parent="Widget.TitlePageIndicator">
    </style>

    <style name="Widget.MissionHub.VPI.CirclePageIndicator" parent="Widget.CirclePageIndicator">
    </style>
    
    <style name="Widget.MissionHub.VPI.TabPageIndicator" parent="Widget.TabPageIndicator">
        <!-- <item name="android:background">@drawable/tab_indicator</item> -->
    </style>
    <style name="Widget.MissionHub.MissionHub.VPI.TabPageIndicator.Text" parent="Widget.TabPageIndicator.Text">
        <item name="android:textColor">@color/black</item>
    </style>

</resources>